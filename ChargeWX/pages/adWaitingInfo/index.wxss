/* pages/report/index.wxss */
.table {
  border: 1px solid rgb(10, 10, 10);
}
.v2 {
  display: block;
  position: relative;
  width: 100%;
  background-color: rgb(196, 240, 206);
}
.title1{
  text-align: center;
  font-weight: bolder;
  font-size:40rpx;
  font-family: Helvetica;
  color: rgb(100, 0, 17);
  display: flex;
  align-items: center;
  justify-content: center;
}
.td {
  width:40%;
  justify-content: center;
  text-align: center;
}
.td1 {
  width:60%;
  justify-content: center;
  text-align: center;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 1.4rem;
  align-items: center;
  border-bottom: 1rpx solid #000000;
  border-left: 1rpx solid #000000;
  
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
